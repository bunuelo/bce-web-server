{
    "name": "bce-web-server",
    "version": "0.0.1",
    "private": true,
    "scripts": {
	"dev": "source ../.env && vite dev --port $(BCE_WEB_SERVER_HTTPS_PORT)",
	"build": "vite build",
	"preview": "source ../.env && vite preview --port $(BCE_WEB_SERVER_HTTPS_PORT)",
	"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
	"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch"
    },
    "devDependencies": {
	"@sveltejs/adapter-auto": "^3.0.0",
	"@sveltejs/kit": "^2.0.0",
	"@sveltejs/vite-plugin-svelte": "^3.0.0",
	"svelte": "^4.2.7",
	"svelte-check": "^3.6.0",
	"typescript": "^5.0.0",
	"vite": "^5.0.3",
	"svelte-headlessui": "",
	"svelte-transition": "",
	"three": "*"
    },
    "type": "module"
}
